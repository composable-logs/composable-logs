// See, https://code.visualstudio.com/docs/remote/containers
{
  "name": "composable-logs dev",
  "dockerComposeFile": [
    ".devcontainer-docker-compose.yml",
    "docker/dev-grafana-tempo/docker-compose.yaml"
  ],
  "service": "dev-environment",
  "forwardPorts": [
    3000,
    8888
  ],
  "remoteEnv": {},
  "extensions": [
    "ms-python.python"
  ],
  "workspaceFolder": "/home/host_user/workspace",
  "portsAttributes": {
    "3000": {
      "label": "grafana"
    }
  }
}
